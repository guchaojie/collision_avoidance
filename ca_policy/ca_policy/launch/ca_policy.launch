<launch>

  <!-- vision object -->
 
  <node pkg="ca_policy" name="vision_object" type="vision_object" output="screen">
    <rosparam file="$(find ca_policy)/param/vision_object.yaml" command="load" />
  </node>

  
  <!-- ca policy -->
  <node pkg="ca_policy" name="ca_policy" type="ca_policy" output="screen">
    <rosparam file="$(find ca_policy)/param/ca_policy.yaml" command="load" />
  </node>
</launch>

